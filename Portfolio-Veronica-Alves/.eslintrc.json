{
  "env": { 
    "browser": true, 
    "es2020": true 
  },
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "plugin:react-hooks/recommended",
    "plugin:prettier/recommended",
    "prettier"
  ],
  "parserOptions": { 
    "ecmaVersion": "latest", 
    "sourceType": "module" 
  },
  "settings": { "react": { "version": "18.2" } },
  "plugins": [
    "react",
    "react-hooks",
    "prettier"
  ],
  "rules": {
    "react/jsx-filename-extension": [0]
  }
}
